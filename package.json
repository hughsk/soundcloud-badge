{
  "name": "soundcloud-badge",
  "version": "1.0.0",
  "description": "A SoundCloud 'now-playing' badge that you can just drop into browserify demos",
  "main": "index.js",
  "dependencies": {
    "brfs": "~0.0.8",
    "google-fonts": "~0.0.0",
    "insert-css": "~0.0.0",
    "minstache": "~1.1.0",
    "soundcloud-resolve": "^1.0.2"
  },
  "browserify": {
    "transform": "brfs"
  },
  "devDependencies": {},
  "scripts": {
